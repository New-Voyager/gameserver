    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Scheduler",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "API_SERVER_INTERNAL_URL": "http://localhost:9502",
                "LOG_LEVEL": "debug",
                "POST_PROCESSING_TIMEOUT_SEC": 120,
                "POST_PROCESSING_INTERVAL_SEC": 60,
                "EXPIRE_GAMES_INTERVAL_SEC": 10,
                "EXPIRE_GAMES_TIMEOUT_SEC": 30,
                "DATA_RETENTION_INTERVAL_MIN": 60,
                "DATA_RETENTION_TIMEOUT_MIN": 10,
            }
        }
    ]
}